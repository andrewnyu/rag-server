# RAG Server configuration - Example file
# Copy this file to .env and update with your values

# Git settings
GIT_REPO_URL=git@github.com:andrewnyu/rag-server.git
GIT_BRANCH=main
GIT_DEPLOY_KEY_PATH=~/.ssh/rag_deploy_key

# Server settings
SERVER_PORT=8000
SERVER_HOST=0.0.0.0

# RAG settings
DEFAULT_DOCS_FOLDER=./default_docs

# LLM endpoint
LLM_ENDPOINT_URL=https://your-llm-endpoint-url/query?text= 